[alias]
	b  = branch
	co = checkout

	d  = diff
	ds = d --staged

	s  = status --short
	ss = status-short-staged
	sr = status-short-red

	ls  = ls-files
	lsu = ls --others --exclude-standard

	l  = log --oneline --max-count 10
	ln  = l --name-status
	la = l --max-count -1
	lg = la --graph --all

	amend  = commit --amend --no-edit

	abort    = rebase --abort
	continue = rebase --continue

	unstage = reset --quiet
