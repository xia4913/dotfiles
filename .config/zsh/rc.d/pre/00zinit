local zinit_dir=
for zinit_dir in /usr/share/zinit ~/lib/zsh/zinit; do
    local f=$zinit_dir/zinit.zsh
    if [[ -f $f ]]; then
        . $f
        break
    fi
done
load-rc $(files-in-dir $ZDOTDIR/zinit/rc.d)

autoload -Uz _zinit
(( ${+_comps} )) && _comps[zinit]=_zinit

local k= v=
alias | while IFS== read -r k v; do
    if [[ $v = zinit ]]; then
        unalias $k
    fi
done
